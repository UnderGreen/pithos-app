[connection]
hostname = cassandra.default.svc.cluster.local
port = 9042
ssl = true
factory = cqlshlib.ssl.ssl_transport_factory

[ssl]
certfile = /etc/ssl/cassandra/cassandra-node.pem
validate = true
userkey = /etc/ssl/cassandra/cassandra-node.pem
usercert = /etc/ssl/cassandra/cassandra-node.pem
version = TLSv1_2
